public class Result {

	
	private String code ;  // 10001 成功   10002 失败
	
	private String msg;
	
	private Object data;

	
	
	
	public void setInfo(String code, String msg, Object data) {
		this.code = code;
		this.msg = msg;
		this.data = data;
	}

	public void setInfo(String code,String msg){
		this.code = code;
		this.msg= msg;
	}
	
	public Result(){
		code = "10001";
		msg = "操作成功";
	}
	
	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public Object getData() {
		return data;
	}

	public void setData(Object data) {
		this.data = data;
	}
	
	
}
